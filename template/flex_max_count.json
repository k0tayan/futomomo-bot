{
  "type": "bubble",
  "styles": {
    "header": {
      "backgroundColor": "#F1789B"
    },
    "body": {
      "backgroundColor": "#ffffff"
    },
    "footer": {
      "backgroundColor": "#F1789B"
    }
  },
  "header": {
    "type": "box",
    "layout": "vertical",
    "contents": [
      {
        "type": "spacer",
        "size": "xs"
      }
    ]
  },
  "body": {
    "type": "box",
